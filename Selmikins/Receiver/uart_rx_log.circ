--&#13;
--------------------------------------------------------------------------------&#13;
-- Description :&#13;
--&#13;
--------------------------------------------------------------------------------&#13;
&#13;
LIBRARY ieee;&#13;
USE ieee.std_logic_1164.all;&#13;
USE ieee.numeric_std.all;
&#13;
ENTITY clock_tester IS&#13;
  PORT (&#13;
  ------------------------------------------------------------------------------&#13;
  --Insert input ports below&#13;
    clock      : IN  std_logic;                    -- input bit example&#13;
  ------------------------------------------------------------------------------&#13;
  --Insert output ports below&#13;
    freq_signal        : OUT std_logic                    -- output bit example&#13;
    );&#13;
END clock_tester;&#13;
&#13;
--------------------------------------------------------------------------------&#13;
--Complete your VHDL description below&#13;
--------------------------------------------------------------------------------&#13;
ARCHITECTURE TypeArchitecture OF clock_tester IS
    type state_type is (STATE_LOW, STATE_HIGH);
    signal clock_state : state_type := STATE_LOW;  -- Set initial state to STATE_LOW
    signal r_Clock_Test : unsigned(7 downto 0) := (others =&gt; '0');
    constant CLKS_PER_BIT : integer := 12500000; --clk freq = 6.25MHz / CLKS_PER_BIT = BR of 1

BEGIN

    process(clock) begin
        if rising_edge(clock) then
            case clock_state is
                when STATE_LOW =&gt;
                    if r_Clock_Test = CLKS_PER_BIT-1 then
                        clock_state &lt;= STATE_HIGH;
                        r_Clock_Test &lt;= (others =&gt; '0'); 
                      --  freq_signal &lt;= '1';
                    else
                    	clock_state &lt;= STATE_LOW;
                        	r_Clock_Test &lt;= r_Clock_Test + 1;
                        	freq_signal &lt;= '0';
                    end if;
                when STATE_HIGH =&gt;
                    if r_Clock_Test = CLKS_PER_BIT-1 then
                        clock_state &lt;= STATE_LOW;
                        r_Clock_Test &lt;= (others =&gt; '0');
                      --  freq_signal &lt;= '0';
                    else
                    	clock_state &lt;= STATE_HIGH;
                        	r_Clock_Test &lt;= r_Clock_Test + 1;
                        	freq_signal &lt;= '1';
                    end if;
            end case;
        end if;
        
        if clock_state = STATE_LOW then
        	freq_signal &lt;= '0';
        else 
        	freq_signal &lt;= '1';
        end if;
        
    end process;

END TypeArchitecture;


</vhdl>
